@import ./components/header.gspec

@objects
    middle              id      middle
    content             id      content
    aside               id      side-panel

= Main section =
    content:
        height 220px
        inside middle
        @on desktop
            width 724px
        @on tablet
            width 340px
        @on mobile
            width 320px
    aside:
        height 220px
        inside middle
        @on desktop
            width 300px
        @on tablet
            width 300px
        @on mobile
            width 320px
